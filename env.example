# RSA Insurance Management System - Environment Variables Example
# Copy this file to .env.local for local development
# For Vercel deployment, add these variables in the Vercel dashboard

# Backend API URL (if you have a separate backend)
VITE_APP_BACKEND_URL=https://your-backend-api.vercel.app

# Firebase Configuration (stringified JSON)
VITE_APP_FIREBASE_CONFIG={"apiKey":"your-api-key","authDomain":"your-project.firebaseapp.com","projectId":"your-project-id","storageBucket":"your-project.appspot.com","messagingSenderId":"123456789","appId":"your-app-id"}

# Razorpay Configuration
VITE_APP_RAZORPAY_KEY_ID=rzp_test_your_key_here

# EmailJS Configuration
VITE_APP_EMAILJS_USER_ID=user_abc123def456

# Optional: Initial Auth Token
VITE_APP_INITIAL_AUTH_TOKEN=your_initial_token_here 